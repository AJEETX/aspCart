@model IEnumerable<aspCart.Web.Areas.Admin.Models.Catalog.ProductCreateOrUpdateModel>

<ul class="navbar-nav">
    <li class="nav-item dropdown">
        <a class="nav-link dropdown-toggle my-1" href="#" id="categoryDropdown" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
            Pages
        </a>
        <div class="dropdown-menu" aria-labelledby="categoryDropdown">
            @foreach (var category in Model)
            {
                <a asp-area="" asp-controller="About" asp-action="Index" class="dropdown-item">@category.Name</a>
            }
        </div>
    </li>
</ul>
